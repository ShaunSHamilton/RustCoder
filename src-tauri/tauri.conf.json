{
  "build": {
    "distDir": "../public",
    "devPath": "http://localhost:8420",
    "beforeDevCommand": "",
    "beforeBuildCommand": "",
    "withGlobalTauri": true
  },
  "ctx": {},
  "tauri": {
    "embeddedServer": {
      "active": false
    },
    "bundle": {
      "active": true,
      "targets": "msi",
      "identifier": "com.tauri.dev",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "resources": ["../server-dist/bundle_server.js"],
      "externalBin": [],
      "copyright": "",
      "category": "Education",
      "shortDescription": "A place to learn Rust",
      "longDescription": "A stand-alone application to teach Rust from the beginning",
      "deb": {
        "depends": [],
        "useBootstrapper": false
      },
      "osx": {
        "frameworks": [],
        "minimumSystemVersion": "",
        "useBootstrapper": false
      },
      "exceptionDomain": ""
    },
    "allowlist": {
      "all": true
    },
    "window": {
      "title": "RustCoder",
      "width": 800,
      "height": 600,
      "resizable": true,
      "fullscreen": true
    },
    "security": {
      "csp": "default-src blob: data: filesystem: ws: http: https: 'unsafe-eval' 'unsafe-inline'"
    },
    "inliner": {
      "active": true
    }
  }
}
